									"                            type: \"string\"",
									"                        },",
									"                        name: {",
									"                            type: \"string\"",
									"                        },",
									"                        externalId: {",
									"                            type: \"string\"",
									"                        },",
									"                        createdAt: {",
									"                            type: \"string\"",
									"                        },",
									"                        updatedAt: {",
									"                            type: \"string\"",
									"                        },",
									"                    },",
									"                    required: [\"id\", \"name\", \"externalId\", \"createdAt\", \"updatedAt\"]",
									"                }",
									"            },",
									"            required: [\"createUser\"]",
									"        },",
									"        required: [\"data\"]",
									"    }",
									"};",
									"",
									"pm.test('Schema is valid', function () {",
